							END_IF
				END_IF
				
//				IF bBottle1 AND NOT bBottle2 THEN
//					bBottle1:=FALSE; bSensor1:=FALSE; bBottle2:=TRUE; //this simulates the conveyor!
//				END_IF
	
				CASE iState OF
				0: IF bBottle2 THEN
					iState:=10;
					END_IF
				
				10: //we just got a bottle, let's fill it!
