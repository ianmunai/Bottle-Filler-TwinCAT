							fbMyTimer2(in:=FALSE);
							bBottle3:=FALSE;
							bSensor3:=FALSE;
							bBottle1:=TRUE;				
							iState:=10;
						END_IF
				END_CASE 
				
	
END_IF
(*.....note this commented endif is to try out the auto and man lights.....*)


IF bManualMode THEN
		bManualLight:=TRUE;
		bAutoLight:=FALSE;
		bAutoMode:=FALSE;
	
	IF bReverseMode THEN
		 
		bManualMode:=TRUE;
